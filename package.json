{
  "name": "react-babel-webpack-boilerplate",
  "version": "1.4.1",
  "description": "A boilerplate for React-Babel-Webpack project",
  "main": "app/main.jsx",
  "scripts": {
    "storybook": "start-storybook -p 8001 -c .storybook",
    "precommit": "npm run lint && jest",
    "commitmsg": "npm run lint:msg",
    "lint": "eslint --cache --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "lint:msg": "validate-commit-msg",
    "test": "npm run lint && jest",
    "build": "webpack",
    "start": "webpack-dev-server --progress --hot --content-base app",
    "start:prod": "node server.bundle.js",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy:server": "NODE_ENV=production webpack --config webpack.server.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "rancher-dev-upgrade": "node .scripts/rancher-dev-upgrade.js"
  },
  "dependencies": {
    "apollo-client": "^0.8.7",
    "auth0-lock": "^10.11.0",
    "babel-runtime": "6.x",
    "file-loader": "^0.10.0",
    "font-awesome": "^4.7.0",
    "glamor": "^2.20.24",
    "glamorous": "^3.11.1",
    "graphql-tag": "^1.2.4",
    "history": "^4.6.1",
    "jwt-decode": "^2.1.0",
    "moment": "^2.17.1",
    "node-sass": "^4.5.0",
    "react": "15.x",
    "react-addons-css-transition-group": "^15.4.2",
    "react-apollo": "^0.11.2",
    "react-dom": "15.x",
    "react-ga": "^2.1.2",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.1.1",
    "recharts": "^0.20.8",
    "redux": "^3.6.0",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "alpha",
    "@storybook/react": "alpha",
    "babel-core": "6.x",
    "babel-eslint": "7.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "1.x",
    "copy-webpack-plugin": "latest",
    "css-loader": "~0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-compat": "^1.0.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "3.x",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "latest",
    "husky": "^0.12.0",
    "jest": "^20.0.3",
    "open-browser-webpack-plugin": "0.0.3",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.1",
    "style-loader": "~0.13.1",
    "validate-commit-msg": "2.x",
    "webpack": "latest",
    "webpack-dev-server": "latest"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Brian Bartholomew",
  "license": "No License",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
